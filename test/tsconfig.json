{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "es5",
        "lib": ["es2015", "DOM", "ES2019.Object"],
        "esModuleInterop": true,
        "declaration": true,
        "outDir": "../dist_test",
        "sourceMap": true,
        "newLine": "LF",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "incremental": true,
        "strict": true,
        "downlevelIteration": true,
        "jsx": "react",
        "noFallthroughCasesInSwitch": true
    },
    "include": ["../src", "."]
}
