{
    "name": "keycloakify",
    "version": "2.4.0",
    "description": "Keycloak theme generator for Reacts app",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/keycloakify.git"
    },
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "clean": "rimraf dist/",
        "build": "yarn clean && tsc && yarn grant-exec-perms && yarn copy-files",
        "grant-exec-perms": "node dist/bin/tools/grant-exec-perms.js",
        "test": "node dist/test/bin/main && node dist/test/lib",
        "copy-files": "copyfiles -u 1 src/**/*.ftl src/**/*.xml src/**/*.js dist/",
        "generate-messages": "node dist/bin/generate-i18n-messages.js",
        "link_in_test_app": "node dist/bin/link_in_test_app.js"
    },
    "bin": {
        "build-keycloak-theme": "dist/bin/build-keycloak-theme/index.js",
        "download-builtin-keycloak-theme": "dist/bin/download-builtin-keycloak-theme.js"
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [
        "bluehats",
        "keycloak",
        "react",
        "theme",
        "FreeMarker",
        "ftl",
        "login",
        "register"
    ],
    "homepage": "https://github.com/garronej/keycloakify",
    "devDependencies": {
        "@types/node": "^10.0.0",
        "@types/react": "^17.0.0",
        "copyfiles": "^2.4.1",
        "properties-parser": "^0.3.1",
        "react": "^17.0.1",
        "rimraf": "^3.0.2",
        "ts-toolbelt": "^9.6.0",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "cheerio": "^1.0.0-rc.5",
        "evt": "2.0.0-beta.38",
        "minimal-polyfills": "^2.2.1",
        "path": "^0.12.7",
        "powerhooks": "^0.9.3",
        "react-markdown": "^5.0.3",
        "scripting-tools": "^0.19.13",
        "tsafe": "^0.4.1",
        "tss-react": "^0.9.1"
    }
}
